{
    "panelType": "v2board",
    "panels": {
        "AfeiCloud": [
            {
                "url": "https://v2board.afeicloud.de",
                "description": "AfeiCloud 主面板"
            }
        ]
    },
    "proxy": [],
    "ws": [],
    "update": [],
    "onlineSupport": [],
    "subscription": {
        "urls": [
            {
                "url": "https://v2board.afeicloud.de",
                "description": "AfeiCloud 订阅",
                "supportEncrypt": true,
                "endpoints": {
                    "v1": {
                        "path": "/api/v1/client/subscribe?token={token}",
                        "requiresToken": true,
                        "method": "GET",
                        "description": "V1 普通订阅接口",
                        "isEncrypt": false
                    },
                    "v2": {
                        "path": "/api/v2/subscription-encrypt/{token}",
                        "requiresToken": true,
                        "method": "GET",
                        "description": "V2 加密订阅接口",
                        "isEncrypt": true
                    }
                }
            }
        ],
        "enableEncrypt": true,
        "preferredEncryptVersion": "v2"
    }
}
